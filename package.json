{
  "name": "ictools-config-analyzer",
  "displayName": "ICTools Config Analyzer",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.105.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:ictools-config-analyzer.openWebUI"
  ],
  "main": "./out/extension.js",
    "contributes": {
      "commands": [
        {
          "command": "ictools-config-analyzer.openWebUI",
          "title": "ICTools: Open Config Analyzer UI"
        }
      ],
      "releaseNotes": {
        "0.0.1": "Initial release: Source Code and Pip Config validation"
      }
    }
,
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.105.0",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.39.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.3"
  },
  "dependencies": {
    "yaml": "^2.8.1"
  }
}
